{
  "name": "sim-racer-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts --respawn",
    "start:dev": "ts-node-dev src/server.ts --respawn --inspect=4321",
    "test": "jest --passWithNoTests --runInBand --silent",
    "test:unit": "yarn test --watch -c jest-unit-config.js",
    "test:unit:verbose": "yarn jest --passWithNoTests --runInBand --watch -c jest-unit-config.js",
    "test:integration": "yarn test --watch -c jest-integration-config.js",
    "test:integration:verbose": "yarn jest --passWithNoTests --runInBand --watch -c jest-integration-config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "mongodb:seed": "ts-node -r dotenv/config seeds/mongodb/mongodb.seed.ts",
    "mongodb:start": "./src/scripts/start-mongo.sh",
    "husky:prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "16"
  },
