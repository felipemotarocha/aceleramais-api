    "jest": "^27.4.7",
    "lint-staged": "^12.2.2",
    "mongo-seeding": "^3.7.1",
    "nodemon": "^2.0.15",
    "prettier-config-standard": "^4.0.0",
    "sucrase": "^3.20.3",
